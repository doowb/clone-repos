{
  "name": "clone-repos",
  "description": "Clone all of a user's github repositories.",
  "version": "0.1.3",
  "homepage": "https://github.com/doowb/clone-repos",
  "author": "Brian Woodward (https://github.com/doowb)",
  "repository": "doowb/clone-repos",
  "bugs": {
    "url": "https://github.com/doowb/clone-repos/issues"
  },
  "license": "MIT",
  "files": [
    "bin",
    "index.js",
    "utils.js"
  ],
  "main": "index.js",
  "bin": {
    "clone-repos": "./bin/cli.js"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "async": "^1.5.2",
    "github-base": "^0.4.1",
    "lazy-cache": "^2.0.1",
    "minimist": "^1.2.0",
    "minimist-events": "^0.1.3",
    "mkdirp": "^0.5.1",
    "union-value": "^0.2.3"
  },
  "devDependencies": {
    "delete": "^0.3.2",
    "gulp-format-md": "^0.1.10",
    "mocha": "^3.0.2"
  },
  "keywords": [
    "clone",
    "github",
    "repos",
    "repositories"
  ],
  "verb": {
    "related": {
      "list": [
        "github-base"
      ]
    },
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "verb",
      "verb-generate-readme"
    ]
  }
}
